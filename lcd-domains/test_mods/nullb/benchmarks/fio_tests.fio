[global]
direct=1 
filename=/dev/nullb0
numjobs=1
iodepth=4
ioengine=libaio
size=2G
rw=randrw

; Synchronous reads
[sync-read-4k]
rw=read
bs=4k

[sync-write-4k]
rw=write
bs=4k

; Varying block sizes

[block-512]
bs=512

[block-4k]
bs=4k

[block-1M]
bs=1M

; Only applicable for async rw

[vary-iodepth1]
bs=4k
iodepth=8

[vary-iodepth2]
bs=4k
iodepth=16

[vary-iodepth3]
bs=4k
iodepth=32

[seq-rw-block-512]
bs=512
rw=readwrite

[seq-rw-block-4k]
bs=4k
rw=readwrite

[seq-rw-block-1M]
bs=1M
rw=readwrite
